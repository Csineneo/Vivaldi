include_rules = [
    # Bind/Callback is only allowed in rare cases, here it's allowed to convert
    # from WTF::Function to base::Callback in order to pass callbacks out of
    # blink.
    "+base/bind.h",
    "+base/callback.h",
    "+base/callback_helpers.h",
    "+base/logging.h",
    "+base/macros.h",
    "+base/memory/ptr_util.h",
]
