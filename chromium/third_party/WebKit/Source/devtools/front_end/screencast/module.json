{
    "extensions": [
        {
            "type": "@WebInspector.AppProvider",
            "className": "WebInspector.ScreencastAppProvider",
            "order": 1
        },
        {
            "type": "@WebInspector.ToolbarItem.Provider",
            "className": "WebInspector.ScreencastApp.ToolbarButtonProvider",
            "order": 1,
            "location": "main-toolbar-left"
        },
        {
            "type": "@WebInspector.ActionDelegate",
            "actionId": "screencast.request-app-banner",
            "className": "WebInspector.ScreencastApp.ActionDelegate",
            "title": "Request app banner\u2026"
        },
        {
            "type": "context-menu-item",
            "condition": "remoteFrontend",
            "location": "mainMenu/tools",
            "experiment": "appBanner",
            "order": 10,
            "actionId": "screencast.request-app-banner"
        }
    ],
    "dependencies": [
        "components"
    ],
    "scripts": [
        "ScreencastApp.js",
        "ScreencastView.js"
    ],
    "resources": [
        "screencastView.css"
    ]
}
