{
  "manifest_version":  1,
  "name": "service:tracing",
  "display_name": "Tracing Collector",
  "capabilities": {
    "provided": {
      "app": [
        "tracing::mojom::Factory",
        "tracing::mojom::Collector",
        "tracing::mojom::StartupPerformanceDataCollector"
      ]
    },
    "required": {
      "service:shell": { "classes": [ "shell:all_users" ] }
    }
  }
}
