{
  "manifest_version":  1,
  "name": "mojo:ash_sysui",
  "display_name": "System UI",
  "capabilities": {
    "required": {
      "*": { "classes": [ "app" ] },
      "mojo:desktop_wm": {
        "interfaces": [
          "mash::wm::mojom::ShelfLayout",
          "mash::wm::mojom::UserWindowController"
        ]
      },
      "exe:chrome" : { "classes": [ "app_list:presenter" ] }
    }
  }
}
