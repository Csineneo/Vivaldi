{
  "manifest_version": 1,
  "name": "mojo:connect_test_app",
  "display_name": "Connect Test App",
  "capabilities": {
    "required": {
      "mojo:connect_test_a": { "interfaces": ["*"] },
      "mojo:connect_test_class_app": {
