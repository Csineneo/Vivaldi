{
  "manifest_version":  1,
  "name": "service:quick_launch",
  "display_name": "Quick Launch Bar",
  "capabilities": {
    "provided": { "mash:launchable": [ "mash::mojom::Launchable" ] },
    "required": {
      "*": { "classes": [ "app", "mash:launchable" ] },
      "service:catalog": { "interfaces": [ "catalog::mojom::Catalog" ] }
    }
  }
}
