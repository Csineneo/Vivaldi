{
  "owners": [
    "dpranke",
    "smut"
  ],
  "comments": [
    "GN + Ninja simulator build."
  ],
  "xcode version": "7.0",
  "GYP_DEFINES": {
    "chromium_ios_signing": "0",
    "gomadir": "$(goma_dir)",
    "target_subarch": "both",
    "use_goma": "1"
  },
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "mb_type": "gn",
  "compiler": "ninja",
  "additional_compile_targets": ["gn_all"],
  "configuration": "Debug",
  "sdk": "iphonesimulator9.0",
  "tests": [
  ]
}
